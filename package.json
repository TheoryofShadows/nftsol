{
  "name": "nftsol",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n server,client -c auto \"npm run dev:server\" \"npm run dev:client\"",
    "api:dev": "cd backend-advanced && node server/index.js",
    "web:dev": "cd frontend && vite",
    "anchor:build": "bash -lc 'cd anchor/solana_rewards && anchor build'",
    "anchor:generate": "node scripts/generate-anchor-clients.mjs",
    "prebuild": "npm run anchor:build || echo 'skip anchor build'; npm run anchor:generate || echo 'skip anchor generate'",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev:server": "tsx --env-file=.env --trace-uncaught server/index.ts",
    "dev:client": "vite --config vite.config.client.local.js --host 127.0.0.1 --port 5173",
    "postinstall": "patch-package"
  },
  "overrides": {
    "bigint-buffer": "1.1.5"
  },
